    <main class="d-flex flex-nowrap">
      <%- include('../Components/sidebar.ejs', {currentPage: "add"}) %>

      <section class="w-100">
        <%- include('../Components/navbar.ejs') %>

        <form  action="/user/add.html" method="post" class="row gx-3 gy-4 mt-1 mx-0">
    
          <div class="col-md-6">
            <label for="inputfirstname" class="form-label">First Name</label>
            <input type="text" class="form-control" id="inputfirstname" placeholder="yosif" name="firseName">
          </div>
          <div class="col-md-6">
            <label for="inputlastname" class="form-label">Last Name</label>
            <input type="text" class="form-control" id="inputlastname" placeholder="Hamid Al-din" name="lastName">
          </div>
      
      
      
          <div class="col-md-6">
            <label for="inputemail" class="form-label">Email</label>
            <input type="email" class="form-control" id="inputemail" placeholder="yosif@gmail.com" name="email">
          </div>
          <div class="col-md-6">
            <label for="Telephone" class="form-label">Telephone</label>
            <input type="number" class="form-control" id="Telephone" placeholder="7------18" name="phoneNumber">
          </div>
      
          <div class="col-md-6">
